{
  "title": "ffmetadata",
  "name": "ffmetadata",
  "version": "0.0.2",
  "description": "A basic add-on",
  "main": "index.js",
  "author": "",
  "engines": {
    "firefox": ">=38.0a1",
    "fennec": ">=38.0a1"
  },
  "license": "MIT",
  "keywords": [
    "jetpack"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "webpack && cpx src/static/**/* data && node bin/replace-html",
    "start:js": "webpack -w",
    "start:static": "cpx src/static/**/* data -w",
    "start:server": "live-server data --port=1936 --no-browser",
    "start": "npm-run-all --parallel start:*",
    "addon": "npm run build && jpm run",
    "publish": "npm run build && jpm sign --api-key ${AMO_API_KEY} --api-secret ${AMO_API_SECRET}"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-react": "^6.5.0",
    "cpx": "^1.3.1",
    "jpm": "^1.0.7",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "live-server": "^1.0.0",
    "mocha": "^2.4.5",
    "npm-run-all": "^2.3.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "webpack": "^1.13.0",
    "webpack-notifier": "^1.3.0"
  },
  "dependencies": {
    "jsdom": "^9.0.0",
    "page-metadata-parser": "^0.2.0",
    "url": "^0.11.0",
    "url-parse": "^1.1.1"
  }
}
